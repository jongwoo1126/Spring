<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.ch06.persistence.MemberDao">
	<insert id="insertMember">
		INSERT INTO `Member` VALUES (#{uid}, #{name}, #{hp}, #{dep}, #{pos}, #{rdate});
	</insert>
	<select id="selectMember" resultType="kr.co.ch06.vo.MemberVo"></select>	
	<select id="selectMembers" resultType="kr.co.ch06.vo.MemberVo"></select>
	<update id="updateMember"></update>
	<delete id="deleteMember"></delete>	
</mapper>